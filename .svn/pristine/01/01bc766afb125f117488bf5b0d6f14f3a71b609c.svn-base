  </select>
  <update id="updateReporStatus" >
    UPDATE WX_HOUSEKEEPER_REPORT T
    SET T .IS_EXPIRED = 1
    WHERE
    T .IS_EXPIRED = 0
    AND T .status = 0
    AND ADD_MONTHS (T .SUBSCRIBE_DATE, T .PERIOD) &lt;= SYSDATE
  </update>
