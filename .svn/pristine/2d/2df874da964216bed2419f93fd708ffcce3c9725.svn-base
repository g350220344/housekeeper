g('path:  ', path)\nif(path === '') {\n\n}else {\nthis.$router.push({path: path, query: {'communityId': this.communityId}})\n}\n},\nselReport(menuKey, menuItem) {\nif(menuKey == 'cancel') {\nreturn;\n}\nlet pathArr = ['', '', '/reportBrief', '/reportSell', '', ''];\nif(menuItem.isRead == 0) {\nthis.setRead(menuItem.value).then(() => {\nthis.checkReport(pathArr[menuItem.r_type]);\n}).catch(err => {\n\n})\n}else {\nthis.checkReport(pathArr[menuItem.r_type]);\n}\n},\nsetRead(id) {\nthis.$store.commit('updateLoadingStatus', {isLoading: true})\nreturn new Promise((resolve, reject) => {\nthis.$http({\nurl: 'WxReport/updateReportRead',\nmethod: 'post',\ndata: {\ncommunityId: id\n}\n}).then(res => {\nif(res.data.statusCode == 1) {\nresolve()\n}else {\nreject();\n}\nthis.$store.commit('updateLoadingStatus', {isLoading: false})\n})\n})\n\n},\ngetReportList() {\nthis.$http({\nurl: 'WxUser/selectReportByUserIdAndCommunityId',\nmethod: 'post',\ndata: {\ncommunityId: this.communityId\n}\n}).then(res => {\nlet data = res.data.data;\nlet arr = ['', '完整报告', '简要报告', '出售报告', '出租报告', '持产报告'];\nif(res.data.statusCode == 1) {\nthis.reportList = data.map(item => {\nreturn {\nlabel: item.isExpired == 1 ? arr[item.type] + '(已过期)' : arr[item.type],\nvalue: item.communityId,\nr_type: item.type,\nisExpired: item.isExpired,\nisRead: item.isRead,\ntype: item.isExpired == 1 ? 'disabled' : 'default'\n}\n})\n}\n})\n},\nonPopHide() {\nthis.showBuyPop = false\n},\naddHouse() {\nlet community = {\ncityId: this.info.cityId,\ncityName: this.info.cityName,\ndistrictId: this.info.districtId,\ndistrictName: this.info.districtName,\ncommunityId: this.info.infoId,\ncommunityname: this.info.communityname,\ndetailaddress: this.info.detailaddress,\n\n};\nlet formCity = {\ncityId: this.info.cityId,\ncityName: this.info.cityName,\ndistrictId: this.info.districtId,\ndistrictName: this.info.districtName,\n}\nthis.$store.commit('updateFormCommunityStatus', community)\nthis.$store.commit('updateFormCityStatus', formC