  <select id="getRankByUserId" parameterType="java.lang.String" resultType="java.util.Map" >
 SELECT
	*
FROM
	(
		SELECT
			ROWNUM as rn,
			user_id,
			price,
			(
				SELECT
					COUNT (*)
				FROM
					(
						SELECT
							USER_ID
						FROM
							WX_HOUSEKEEPER_USER_INFO
						GROUP BY
							USER_ID
					)
			) AS COUNT
		FROM
			(
				SELECT
					SUM (
						(
							ASSESS_SALE_PRICE * BUILDINGSQUARE
						)
					) AS price,
					USER_ID
				FROM
					WX_HOUSEKEEPER_HOUSE_INFO
				GROUP BY
					USER_ID
				ORDER BY
					price DESC
			)
	)
WHERE
	 USER_ID = #{userId}



  </select>
  
 
