   <select id="getHouseTopPx"  parameterType="string" resultMap="BaseResultMap" >
  	select t.*,rownum from (
    SELECT
    T .USER_ID,
    nvl( ROUND( SUM (
    T .ASSESS_SALE_PRICE * T .BUILDINGSQUARE
    ) / 10000 ,0),0)
   as price
    FROM
    wx_housekeeper_house_info T
    WHERE
    T .STATUS = 1
    GROUP BY
    T .USER_ID
    ORDER BY
    price desc
    ) t
    ORDER BY
    t.price desc
  </select>
  
  
