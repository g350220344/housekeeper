    </if>
    BUILDINGSQUARE * ASSESS_SALE_PRICE AS ASSESS_SALE_TOTALPRICE,
    (
    SELECT
    NAME
    FROM
    PUB_DICT_VALUE
    WHERE
    TO_CHAR (DICT_VALUE_ID) = ASPECT
    ) as ASPECT_NAME,
    (
    SELECT
    NAME
    FROM
    PUB_DICT_VALUE
