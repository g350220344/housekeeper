  <select id="selectHouseInfoByHouseId" parameterType="Integer" resultMap="BaseResultMap">
  	select *
  	from T_HOUSEKEEPER_HOUSE_INFO
  	where
  	STATUS = 1 AND
  	HOUSE_ID = #{houseId}
  </select>

  <select id="getHouseTop"  resultType="java.util.Map" parameterType="com.cindata.housekeeper.core.entity.Parameter">
    select * from (

    <if test="type == 1">
      SELECT
      c.PROVINCE_NAME as name,
      COUNT (c.PROVINCE_NAME) as count
      FROM
      t_housekeeper_house_info t,"APPLY".CITY_INFO c
      WHERE
      1=1
      <if test="groupId != null and groupId != ''">
        and t.reserve1 = #{groupId,jdbcType=VARCHAR}
      </if>
      and t.CITY_ID=c.CITY_ID
      AND t.status = 1
      GROUP BY
      c.PROVINCE_NAME
      ORDER BY count desc
    </if>

    <if test="type==2">
      SELECT
      c.CITY_NAME as name,
      COUNT (c.CITY_NAME) as count
      FROM
      t_housekeeper_house_info t,"APPLY".CITY_INFO c
      WHERE
      1=1
      <if test="groupId != null and groupId != ''">
        and t.reserve1 = #{groupId,jdbcType=VARCHAR}
      </if>
      and t.CITY_ID=c.CITY_ID
      AND t.status = 1
      GROUP BY
      c.CITY_NAME
      ORDER BY count desc
    </if>
    )
    <if test="topNum != null and topNum != ''">
    where
    ROWNUM &lt; #{topNum}
    </if>
  </select>

  <select id="getHousePriceTop"  resultType="java.util.Map" parameterType="string" >

    select t.* from (
    SELECT
    T .USER_ID,
    nvl( ROUND( SUM (
    T .ASSESS_SALE_PRICE * T .BUILDINGSQUARE
    ) / 10000 ,0),0)
   as price
    FROM
    t_housekeeper_house_info T
    WHERE
    T .STATUS = 1
    <if test="groupId != null and groupId != ''">
      and T .reserve1 = #{groupId}
    </if>
    GROUP BY
    T .USER_ID
    ) t
    ORDER BY
    t.price desc

  </select>
  <select id="getHouseList"  resultMap="BaseResultMap" parameterType="com.cindata.housekeeper.core.entity.Parameter" >
    select *
    from
    (
    select p.*, rownum rn
    from
    (
      SELECT
      T .USER_ID,
      T .ADDRESS,
      T .COMMUNITY_NAME,
      T .DOOR_NUM,
      T .BUILDINGSQUARE,
      T .ASSESS_SALE_PRICE,
      T.BUILDING_ID,
      T.UNIT_ID,
      T .BUILDINGSQUARE * T .ASSESS_SALE_PRICE AS ASSESS_SALE_TOTALPRICE,
      T .CREATED_DT
      FROM
      T_HOUSEKEEPER_HOUSE_INFO T
      WHERE
      T .STATUS = 1
      <if test="groupId != null and groupId != ''">
        AND
        T .RESERVE1 = #{groupId}
      </if>
      <if test="cityId != null and cityId != ''">
        AND
        T .CITY_ID = #{cityId}
      </if>
      <if test="provinceId != null and provinceId != ''">
        AND
        T .RESERVE2 = #{provinceId}
      </if>
      <if test="districtId != null and districtId != ''">
        AND
        T .DISTRICTID = #{districtId}
      </if>
    <if test="userId != null and userId != ''">
      and T.USER_ID like '%' || #{userId} || '%'
    </if>
    <if test="startDate != null and startDate != ''">
      and  T .CREATED_DT &gt;= #{startDate,jdbcType=DATE}
    </if>
    <if test="endDate != null and endDate != ''">
      and  T .CREATED_DT &lt;= #{endDate,jdbcType=DATE}
    </if>
      ORDER BY t.CREATED_DT DESC
    )p
    where rownum &lt;= #{end_no}
    )
    where rn &gt;= #{start_no}

  </select>

  <select id="getHouseListCount" parameterType="com.cindata.housekeeper.core.entity.Parameter" resultType="java.util.Map">

   SELECT
	COUNT (*) as  houseCount,
    nvl(
    SUM (
    T .BUILDINGSQUARE * T .ASSESS_SALE_PRICE
    ),0
    )
	as housePrice,
    nvl(
	MAX (
		(
			SELECT
				COUNT (*)
			FROM
				(
					SELECT
						COUNT (T .USER_ID)
					FROM
						T_HOUSEKEEPER_HOUSE_INFO T
					WHERE
    T .STATUS = 1
    <if test="groupId != null and groupId != ''">
      AND
      T .RESERVE1 = #{groupId}
    </if>
    <if test="cityId != null and cityId != ''">
      AND
      T .CITY_ID = #{cityId}
    </if>
    <if test="provinceId != null and provinceId != ''">
      AND
      T .RESERVE2 = #{provinceId}
    </if>
    <if test="districtId != null and districtId != ''">
      AND
      T .DISTRICTID = #{districtId}
    </if>
    <if test="userId != null and userId != ''">
      AND
      T .USER_ID = #{userId}
    </if>
    <if test="startDate != null and startDate != ''">
      and  T .CREATED_DT &gt;= #{startDate,jdbcType=DATE}
    </if>
    <if test="endDate != null and endDate != ''">
      and  T .CREATED_DT &lt;= #{endDate,jdbcType=DATE}
    </if>
					GROUP BY
						T .USER_ID
				)
		)
	),0
    ) as userCount
FROM
	T_HOUSEKEEPER_HOUSE_INFO T
WHERE
    T .STATUS = 1
    <if test="groupId != null and groupId != ''">
      AND
      T .RESERVE1 = #{groupId}
    </if>
    <if test="cityId != null and cityId != ''">
      AND
      T .CITY_ID = #{cityId}
    </if>
    <if test="provinceId != null and provinceId != ''">
      AND
      T .RESERVE2 = #{provinceId}
    </if>
    <if test="districtId != null and districtId != ''">
      AND
      T .DISTRICTID = #{districtId}
    </if>
    <if test="userId != null and userId != ''">
      AND
      T .USER_ID = #{userId}
    </if>
    <if test="startDate != null and startDate != ''">
