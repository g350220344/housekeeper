/* house page */
.h_bg_block {
    height: 4.68rem;
    background: #388cff;
    color: #fff;
    position: relative;
}
.h_bg_block_menu_btn {
    position: absolute;
    width: 1.12rem;
    height: 0.64rem;
    right: 0;
    top: 0.55rem;
    background-color: rgba(0, 0, 0, 0.3);
    color: #fff;
    text-align: right;
    line-height: 0.64rem;
    border-radius: 0.32rem 0 0 0.32rem;
    z-index: 99;
    font-size: 0.26rem;
    vertical-align: middle;
    font-weight: 300;
    padding-right: 0.05rem;
}
.h_bg_block_menu_btn_icon {
    width: 0.32rem;
    height: 0.28rem;
    vertical-align: middle;
    display: inline-block;
    padding-bottom: 0.08rem;
}
.h_bg_block_row_1 {
    font-size: 0.42rem;
    padding-top: 0.45rem;
    height: 0.4rem;
    line-height: 0.4rem;
    width: 6.67rem;
    margin: auto;
    vertical-align: middle;
}
.h_bg_block_row_1 img {
    width: 0.32rem;
    height: 0.32rem;
    display: inline-block;
    vertical-align: middle;
}
.h_bg_block_row_2 {
    width: 6.67rem;
    /* height: 0.25rem; */
    margin: auto;
    font-size: 0.26rem;
    padding-top: 0.22rem;
}
.h_bg_block_row_3 {
    /* width: 6.67rem; */
    height: 0.63rem;
    line-height: 0.63rem;
    overflow: hidden;
    clear: both;
    margin: auto;
    /* text-align: center; */
    color: #9dc6ff;
    font-size: 0.26rem;
    transform: scale(0.8);
    margin-left: -0.35rem;
}
.h_bg_block_row_3_item {
    float: left;
    width: 33.3%;
    height: 100%;
}
.h_bg_block_row_4 {
    width: 7.02rem;
    margin: auto;
    border-top: 0.01rem solid #afd0ff;
    text-align: center;
    overflow: hidden;
    clear: both;
    /* height: 2.68rem; */
}
.h_bg_block_row_4_item {
    float: left;
    width: 50%;
}
.h_bg_block_row_4_item_label {
    padding-top: 0.36rem;
    font-size: 0.24rem;
    height: 0.23rem;
    line-height: 0.23rem;
}
.h_bg_block_row_4_item_val {
    font-size: 0.52rem;
    line-height: 0.39rem;
    padding-top: 0.15rem;
}
.h_bg_block_row_4_item_rate {
    color: #b5d4ff;
    font-size: 0.24rem;
    padding-top: 0.28rem;
    /* height: 0.23rem; */
    line-height: 0.23rem;
    vertical-align: middle;
}
.h_bg_block_row_4_item_rate img{
    width: 0.13rem;
    height: 0.22rem;
    display: inline-block;
    vertical-align: top;
}
.h_card_1 {
    width: 7.02rem;
    /* height: 6.62rem; */
    margin: auto;
    margin-top: -0.67rem;
    border-radius: 0.3rem;
    background: #fff;
    z-index: 99;
    overflow: hidden;
}
/**重写vux tab 组件样式 **/
.vux-tab-wrap {
    height: 1.04rem!important;
    padding-top: 0!important;
}
.vux-tab-container {
    height: 1.04rem;
}
.vux-tab {
    height: 1.04rem;
}
.vux-tab .vux-tab-item {
    line-height: 1.04rem;
    font-size: 0.3rem;
}
.h_card_1_item {
    position: relative;
}
.h_card_1_item_chart_title {
    position: absolute;
    z-index: 99;
    color: #111;
    font-size: 0.3rem;
    padding-top: 0.2rem;
    left: 0.36rem;
    top: 0;
}
.h_chart_1_lends {
    font-size: 0.22rem;
    color: #555;
    padding-left: 0.9rem;
    padding-bottom: 0.5rem;
}


.h_loan_avilabel_box {
    height: 2.48rem;
    text-align: center;
}
.h_loan_avilabel_box_val {
    padding-top: 0.66rem;
    padding-bottom: 0.29rem;
    font-size: 0.28rem;
    color: #111;
    line-height: 0.34rem;
}
.h_loan_avilabel_box_val span {
    font-size: 0.42rem;
}
.h_loan_apply_btn {
    width: 2.16rem;
    height: 0.64rem;
    line-height: 0.64rem;
    color: #388cff;
    font-size: 0.26rem;
    border-radius: 0.32rem;
    border: solid 1px #388cff;
    margin: auto;
}
.h_distribution_box {
    overflow: hidden;
    clear: both;
}
#h_distribution_chart {
    width: 2.7rem;
    height:3.58rem;
    float: left;
}
.h_distribution_info_block {
    height: 100%;
    width: 3.6rem;
    float: left;
}
.h_distribution_info_block p {
    color: #888;
    font-size: 0.22rem;
    line-height: 0.22rem;
    margin-top: 1rem;
}
.h_distribution_info_block_val {
    padding-top: 0.23rem;
    color: #111;
    font-size: 0.42rem;
}
.h_distribution_info_block_val span {
    font-size: 0.24rem;
}
.h_distribution_chart_lend {
    /* margin-top: 0.25rem;
    font-size: 0.22rem;
    color: #555;
    vertical-align: middle; */

    margin-top: 0.25rem;
    font-size: 0.29rem;
    color: #555;
    vertical-align: middle;
    transform: scale(0.8);
    width: 5rem;
    margin-left: -0.5rem;
}
.h_loan_block_row_1 {
    line-height: 0.28rem;
    padding-top: 0.38rem;
    padding-left: 0.35rem;
    font-size: 0.3rem;
    color: #111;
}
.h_loan_block_row_2 {
    margin-top: 0.68rem;
    line-height: 0.41rem;
    font-size: 0.52rem;
    color: #111;
    text-align: center;
}
.h_loan_block_row_2 span{
    font-size: 0.26rem;
}
.h_loan_block_row_3 {
    font-size: 0.24rem;
    color: #888;
    text-align: center;
    line-height: 0.23rem;
    padding-top: 0.27rem;
    padding-bottom: 0.7rem;
}
.h_loan_card {
    width: 7.02rem;
    height: 3.46rem;
    background: #fff;
    border-radius: 0.3rem;
    margin: 0.24rem auto 0;
}
.h_loan_card_raw_1 {
    overflow: hidden;
    clear: both;
    position: relative;
    padding-bottom: 0.58rem;
}
.h_loan_card_raw_1_left {
    float: left;
    width: 2.12rem;
    height: 100%;
    text-align: center;
}
.h_loan_card_raw_1_left_bank {
    color: #111;
    font-size: 0.3rem;
    /* line-height: 0.28rem; */
}
.h_loan_card_bank_icon {
    width: 0.76rem;
    height: 0.76rem;
    display: block;
    margin: 0.52rem auto 0.15rem;
}
.h_loan_card_raw_1_right {
    width: 4.88rem;
    height: 100%;
    float: left;
}
.h_loan_card_raw_1_divider {
    position: absolute;
    width: 0.02rem;
    height: 1rem;
    background-color: #e3e3e3;
    left: 2.12rem;
    top: 0.68rem;
}
.h_loan_card_raw_1_right_line_label {
    padding-left: 0.3rem;
    padding-top: 0.57rem;
    padding-bottom: 0.2rem;
    line-height: 0.28rem;
    font-size: 0.3rem;
    color: #111;
}
.h_loan_card_raw_1_right_line_val {
    color: #888;
    font-size: 0.24rem;
    padding-left: 0.3rem;
}
.h_loan_clear_btn {
    width: 2.64rem;
    margin: 0.84rem auto 0;
    border-radius: 0.4rem;
    color: #01cfc2;
    border: solid 1px #00d0c2;
    text-align: center;
    line-height: 0.8rem;
    height: 0.8rem;
    font-size: 0.3rem;
}
.h_loan_add_btn {
    width: 100%;
    height: 1rem;
    color: #fff;
    font-size: 0.28rem;
    line-height: 1rem;
    text-align: center;
    background-color: #388cff;
    margin-top: 0.43rem;
}
.h_lease_rate_block {
    width: 100%;
    text-align: center;
    height: 0.72rem;
    overflow: hidden;
    clear: both;
    margin-bottom: 0.46rem;
    position: relative;
}
.h_lease_rate_block_item {
    width: 50%;
    float: left;
}
.h_lease_rate_block_item_val {
    color: #555;
    font-size: 0.3rem;
    padding-bottom: 0.19rem;
    line-height: 0.28rem;
}
.h_lease_rate_block_item_label {
    color: #bbb;
    font-size: 0.24rem;
    line-height: 0.23rem;
}
.h_lease_rate_block_divider {
    position: absolute;
    left: 50%;
    width: 1px;
    height: 0.67rem;
    top: 0;
    background: #e3e3e3;
}
.h_lease_icon_human {
    width: 0.45rem;
    height: 0.66rem;
    display: block;
    margin: 0.51rem auto 0.08rem;
}
.h_lease_icon_phone {
    width: 0.18rem;
    height: 0.18rem;
}
.h_loan_card_raw_1_leaser {
    font-size: 0.26rem;
    color: #111;
    line-height: 0.25rem;
    padding-top: 0.05rem;
}

.h_card_1_item_no_loans {
    width: 100%;
    height: 5.57rem;
    position: relative;
}
.h_card_1_item_no_loans_mask_1 {
    width: 100%;
    /* height: 100%; */
}
.h_show_property_chart_btn {
    width: 3.12rem;
    height: 0.8rem;
    font-size: 0.26rem;
    line-height: 0.8rem;
    color: #fff;
    text-align: center;
    position: absolute;
    background-image: linear-gradient(90deg, 
		#0cc7ff 0%, 
		#5662fc 100%), 
	linear-gradient(
		#388cff, 
        #388cff);
    background-blend-mode: normal, 
		normal;
	box-shadow: 0px 0.12rem 0.12rem 0
		rgba(56, 140, 255, 0.16);
    border-radius: 0.4rem;
    bottom: 2.35rem;
    margin: auto;
    left: 1.98rem;
}
.h_distribution_chart_mask {
    width: 100%;
    height: 3.58rem;
    position: relative;
}
.h_card_1_item_no_loans_2 {
    width: 100%;
    height: 6.67rem;
    text-align: center;
}
.h_card_1_item_no_loans_2_empty_img {
    display: block;
    width: 2.86rem;
    height: 1.66rem;
    margin: auto;
    padding: 1.38rem 0 0.32rem;
}
.h_card_1_item_no_loans_2_p {
    font-size: 0.24rem;
    line-height: 0.23rem;
    color: #bbb;
    text-align: center;
    margin: auto;
}
.h_add_loan_btn {
    width: 2.2rem;
    height: 0.58rem;
    line-height: 0.58rem;
    border: solid 1px #4b9af8;
    color: #388cff;
    font-size: 0.26rem;
    text-align: center;
    border-radius: 0.29rem;
    margin: 0.96rem auto 0;
    vertical-align: middle;
}
.h_add_loan_btn_icon {
    width: 0.24rem;
    height: 0.24rem;
    vertical-align: middle;
    display: inline-block;
    margin-right: 0.05rem;
    padding-bottom: 0.04rem;
}