dotted\",\ncolor: \"#fff\"\n}\n},\nboundaryGap: false\n},\nyAxis: {\ntype: \"value\",\nscale: false,\noffset: 10,\nshow: true,\naxisLabel: {\nshow: true,\ncolor: \"#bbb\"\n},\naxisTick: {\nshow: false\n},\naxisLine: {\nshow: false\n},\nsplitLine: {\nshow: false\n}\n},\nseries: series\n};\n\nmyRegression.setOption(option);\nthis.$nextTick(() => {\nthis.renderQueue.push(index);\n});\n},\nchangeTab(index) {\nthis.tabIndex = index;\nthis.$nextTick(() => {\nthis.renderChart()\n})\n}\n},\nmounted() {\ndocument.title = '简要报告'\n}\n}\n","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"cmt_page\" },\n    [\n      _c(\n        \"div\",\n        { staticClass: \"cmt_pic_wrapper\" },\n        [\n          _c(\"swiper\", {\n            attrs: {\n              list: _vm.pics,\n              \"show-dots\": false,\n              auto: \"\",\n              height: \"4.82rem\"\n            },\n            model: {\n              value: _vm.picIndex,\n              callback: function($$v) {\n                _vm.picIndex = $$v\n              },\n              expression: \"picIndex\"\n            }\n          }),\n          _vm._v(\" \"),\n          _c(\"div\", { staticClass: \"cmt_pic_tips\" }, [\n            _vm._v(_vm._s(_vm.picIndex + 1) + \"/\" + _vm._s(_vm.pics.length))\n          ])\n        ],\n        1\n      ),\n      _vm._v(\" \"),\n      _c(\"div\", { staticClass: \"cmt_block_1\" }, [\n        _c(\"div\", { staticClass: \"cmt_block_1_top vux-1px-b\" }, [\n          _c(\"div\", { staticClass: \"cmt_block_1_top_name\" }, [\n            _vm._v(_vm._s(_vm.community.communityName))\n          ]),\n          _vm._v(\" \"),\n          _c(\"div\", { staticClass: \"cmt_block_1_top_address\" }, [\n            _vm._v(_vm._s(_vm.info.detailaddress || \"\"))\n          ])\n        ]),\n        _vm._v(\" \"),\n        _c(\"div\", { staticClass: \"cmt_block_1_bottom\" }, [\n        