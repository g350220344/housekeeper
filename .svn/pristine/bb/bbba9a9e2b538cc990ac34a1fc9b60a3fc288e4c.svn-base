            width: auto;
            overflow: hidden;
            clear: both;
        }
        /* table style */
        .th-inner:focus {
            outline-width: 0!important;
        }
        .table th:focus {
            outline-width: 0!important;
        }

        table.table thead .both { background: url('img/icon-sort-desc.png') no-repeat; background-position: 95% 50%;}        
        table.table thead .asc { background: url('img/icon-sort-desc.png') no-repeat; background-position: 95% 50%;}
        table.table thead .desc { background: url('img/icon-sort-asc.png') no-repeat; background-position: 95% 50%;}

        .th-label {
            width: 100%;
            text-align: center;
        }
        .th-inner {
            text-align: center;
        }
        td {
            text-align: center;
        }
        .m-b {
            margin-bottom: 0;
            display: inline-block;
            width: 140px;
            margin-left: 10px;
            font-size: 12px;
        }
    </style>
