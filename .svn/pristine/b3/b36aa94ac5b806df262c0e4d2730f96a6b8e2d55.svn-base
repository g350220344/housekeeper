ta: {\nid: this.$route.query.id\n}\n}).then(res => {\nif(res.data.statusCode == 1) {\nthis.hideLoading();\nthis.$router.go(-1);\n}\n})\n},\nonTermChange(item, index) {\nthis.form.cycle = item.id;\n},\nareaCheck(val) {\nif(isNaN(val * 1)) {\nreturn {\nvalid:false, msg:'请输入数字'\n}\n}else {\nreturn {valid:true}\n}\n},\nlog (str1, str2 = '') {\nconsole.log(str1, str2)\n},\nchange() {\n\n},\nonConfirm(val) {\nconsole.log('on-confirm arg', val)\n\n},\nonCancel() {\n\n},\nshowLoading() {\nthis.$store.commit('updateLoadingStatus', {isLoading: true})\n},\nhideLoading(type) {\nthis.$store.commit('updateLoadingStatus', {isLoading: false})\nif(type == 2) {\nthis.$store.commit('updateWarnToastStatus')\n}else {\nthis.$store.commit('updateToastStatus')\n}\n},\nsubmit() {\nif(!this.is_finish) {return}\nvar g = /^[1-9]*[1-9][0-9]*$/;\nlet tel = /^(0\\\\d{2}-\\\\d{8}(-\\\\d{1,4})?)|(0\\\\d{3}-\\\\d{7,8}(-\\\\d{1,4})?)$/; \nlet mobilePartern = /^((13[0-9])|(15[^4])|(18[0,2,3,5-9])|(17[0-8])|(14[5,7]))\\\\d{8}$/; \nvar mode_arr = ['', 1, 3, 12];\nlet mode = mode_arr[this.form.cycle];\nif(!mobilePartern.test(this.form.mobile) && !tel.test(this.form.mobile)) {\nthis.showToast('请输入手机号或固定电话')\nreturn false\n}\nif(!g.test(this.form.money) || this.form.money > 1000000) {\nthis.showToast('请输入正确的租金(1~1000000)')\nreturn false\n}\nif(!g.test(this.form.deposit) || this.form.deposit > 1000000) {\nthis.showToast('请输入正确的押金(1~1000000)')\nreturn false\n}\nif(!g.test(this.form.term) || this.form.term > 500 || this.form.term < mode || this.form.term % mode != 0) {\nthis.showToast('租期不能大于500个月收租周期为“按季”，请输入3的倍数收租周期为“按年”，请输入12的倍数')\nreturn false\n}\nthis.showLoading();\nlet url = 'WxRent/addLease'\nlet data = _.cloneDeep(this.form);\nif(this.is_edit == 1) {\nurl = 'WxRent/updateLease';\ndata['id'] = this.$route.query.id;\n}\nthis.$http({\nurl: url,\nmethod: 'post',\ndata: {\n...data,\nhouseId: this.houseId\n}\n}).then(res => {\nif(res.data.statusCode == 1) {\nthis.hideLoading();\nthis.$router.go(-1);\n}\n})